# CSV FTP Uploader Environment Variables

# Server Configuration
NODE_ENV=production
PORT=5000
HOST=0.0.0.0

# Database Configuration
DATABASE_PATH=/app/data/database.db

# Redis Configuration
REDIS_HOST=redis
REDIS_PORT=6379

# FTP Configuration
FTP_DEFAULT_TIMEOUT=60000
FTP_DEFAULT_RETRY_COUNT=3

# Logging
LOG_LEVEL=info

# Security (Optional)
# SESSION_SECRET=your_session_secret_here